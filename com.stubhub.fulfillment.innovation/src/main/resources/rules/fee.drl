package com.stubhub.drools.entity;dialect  "mvel"rule "Fedx 2D fee"    when       $fugp: FulfillmentUGPermission(fulfillmentWindow.fulfillmentDeliveryMethod.fulfillmentMethod.name=="FEDEX" , fulfillmentWindow.fulfillmentDeliveryMethod.deliveryMethod.name=="FEDEX_2D")    then       $fugp.setDeliveryFee("3,USD");       System.out.println("matching Fedex 2D fee");endrule "Override Fedx 2D  fee" //overide fedex fee for specific genre geo combination    when       $fugp: FulfillmentUGPermission(fulfillmentWindow.fulfillmentDeliveryMethod.fulfillmentMethod.name=="FEDEX" ,fulfillmentWindow.fulfillmentDeliveryMethod.deliveryMethod.name=="FEDEX_2D",genreId==1,geoId==2       )    then       $fugp.setDeliveryFee("12,USD");       System.out.println("matching Override Fedx 2D fee");endrule "Barcode fee"    when       $fugp: FulfillmentUGPermission(fulfillmentWindow.fulfillmentDeliveryMethod.fulfillmentMethod.name=="BARCODE" , fulfillmentWindow.fulfillmentDeliveryMethod.deliveryMethod.name=="INSTANT_DOWNLOAD")    then       $fugp.setDeliveryFee("1.2,USD");       System.out.println("matching Barcode fee");end